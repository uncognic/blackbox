%asm
;tests jz and jnz
%main
    MOV R01, 0
    MOV R05, 0
    CMP R05, R01
    JZ program
    JMP fail

.program:
    WRITE 1  "In program"
    NEWLINE
    MOV R03, 1
    CMP R01, R03
    JNZ success

    JMP fail

.success:
    WRITE 1 "Success"
    NEWLINE
    HALT
    
.fail:
    WRITE 1 "Fail"
    NEWLINE
    HALT