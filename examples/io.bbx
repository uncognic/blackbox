;writes stuff to a file
$main
    FOPEN w, F1, "test"
    FWRITE F1, "123"
    FSEEK F1, 10
    FWRITE F1, "123"
    FSEEK F1, 5
    FWRITE F1, "123"
    FCLOSE F1
    FOPEN r, F1, "test"
