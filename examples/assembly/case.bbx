%asm
;tests case sensitivity for labels and macros
%macro lowercase
    write stdout "lowercase"
    newline
    call lowercase
%endmacro

%macro Lowercase
    write stdout "Lowercase"
    newline
    call Lowercase
%endmacro

%main
    %lowercase
    %Lowercase
    halt ok
    .lowercase:
        write stdout "lowercase label"
        newline
        ret
    .Lowercase:
        write stdout "Lowercase label"
        newline
        ret
    halt ok